    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "validator": "^13.15.15",
    "vue": "^3.5.17",
    "vue-i18n": "^9.14.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.13",
    "@types/validator": "^13.15.3",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.2.1",
